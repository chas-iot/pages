{
    "name": "pages-extension",
    "display_name": "Pages extension",
    "version": "0.0.1",
    "description": "Create and maintain dynamic Pages with different combinations of Things and groups of Things",
    "author": "chas-iot@runegate.org",
    "main": "index.js",
    "scripts": {
